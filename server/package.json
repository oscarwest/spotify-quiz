{
  "author": "Spotify",
  "name": "web-api-auth-examples",
  "description": "Basic examples of the Spotify authorization flows through OAuth 2",
  "version": "0.0.1",
  "scripts": {
    "start": "node dist/app.js",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "debug": "npm run build && npm run watch-debug",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-ts": "tsc -w",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "serve-debug": "cd dist && nodemon --inspect app.js"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "aws-sdk": "^2.188.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "config": "^1.29.4",
    "cookie-parser": "1.3.2",
    "cors": "^2.8.4",
    "errorhandler": "^1.5.0",
    "express": "~4.16.2",
    "express-validator": "^5.0.1",
    "mongodb": "^3.0.2",
    "mongoose": "^5.0.3",
    "querystring": "~0.2.0",
    "redis": "^2.8.0",
    "request": "~2.34.0",
    "request-promise": "^4.2.2",
    "shelljs": "^0.8.1",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.4",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/config": "0.0.33",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/mongodb": "^3.0.5",
    "@types/mongoose": "^4.7.34",
    "@types/node": "^9.4.0",
    "@types/request": "^2.47.0",
    "@types/request-promise": "^4.1.41",
    "@types/shortid": "0.0.29",
    "@types/socket.io": "^1.4.31",
    "@types/uuid": "^3.4.3",
    "nodemon": "^1.14.12",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.5.0"
  }
}
