{
  "author": "Spotify",
  "name": "web-api-auth-examples",
  "description": "Basic examples of the Spotify authorization flows through OAuth 2",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "debug": "npm run build && npm run watch-debug",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-ts": "tsc -w",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "serve-debug": "nodemon --inspect dist/app.js"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "aws-sdk": "^2.185.0",
    "concurrently": "^3.5.1",
    "config": "^1.29.2",
    "cookie-parser": "1.3.2",
    "errorhandler": "^1.5.0",
    "express": "~4.16.2",
    "mongodb": "^3.0.1",
    "mongoose": "^5.0.1",
    "querystring": "~0.2.0",
    "request": "~2.34.0",
    "shelljs": "^0.8.1",
    "socket.io": "^2.0.4",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/config": "0.0.33",
    "@types/cookie-parser": "^1.4.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.0",
    "@types/mongodb": "^3.0.3",
    "@types/mongoose": "^4.7.33",
    "@types/node": "^9.3.0",
    "@types/request": "^2.0.12",
    "@types/socket.io": "^1.4.31",
    "nodemon": "^1.14.11",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.4.2"
  }
}
